{
  "name": "dxf-writer",
  "version": "1.18.4",
  "description": "Dead simple 2D DXF writer",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist --clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "browserify -r ./dist/index.js:Drawing > ./examples/browser/dxf_bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ognjen-petrovic/js-dxf.git"
  },
  "keywords": [
    "DXF",
    "CAD",
    "AutoCAD"
  ],
  "author": "Ognjen Petrovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ognjen-petrovic/js-dxf/issues"
  },
  "homepage": "https://github.com/ognjen-petrovic/js-dxf#readme",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "browserify": "^16.2.3",
    "minimist": ">=1.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
